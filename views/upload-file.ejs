<div class="container">
    <div class="row">
        <% if (records.length > 0) { %>
            <% records.forEach(row => { %>
                <%= row.name %> 
                <img src="./uploads/<%= row.img %>" style="width:300px;height:200px;margin:5px"
                <% }) %>
        <% } %>
    </div>
</div>